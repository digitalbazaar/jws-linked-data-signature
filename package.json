  "devDependencies": {
    "c8": "^7.11.3",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-digitalbazaar": "^3.0.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-unicorn": "^42.0.0",
    "karma": "^6.3.20",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
